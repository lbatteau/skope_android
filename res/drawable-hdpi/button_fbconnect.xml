<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="#23487E" />
            <solid android:color="@color/skope_grey"/>
            <padding android:left="18dip" android:top="8dip"
                android:right="18dip" android:bottom="8dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="#23487E" />
            <solid android:color="#325D9A" />
            <padding android:left="18dip" android:top="8dip"
                android:right="18dip" android:bottom="8dip" />
        </shape>
    </item>
</selector>