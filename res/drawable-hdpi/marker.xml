<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@+id/marker_background">
		<shape android:shape="rectangle">
			<stroke android:width="6dip" android:color="@color/skope_yellow" />
			<solid android:color="@color/skope_grey" />
			<size android:height="34dip" android:width="34dip" />
		</shape>
	</item>
	<item android:id="@+id/marker_thumbnail" android:left="6dip"
		android:top="6dip" android:right="6dip" android:bottom="6dip">
		<shape android:shape="rectangle">
			<solid android:color="@color/skope_grey" />
			<size android:height="20dip" android:width="20dip" />
		</shape>
	</item>
</layer-list>

