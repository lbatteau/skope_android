<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:id="@+id/detail_view" android:orientation="vertical">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_weight="1" android:layout_height="fill_parent"
		android:id="@+id/linearLayout2" android:orientation="horizontal">
		<LinearLayout android:id="@+id/linearLayout5"
			android:layout_height="fill_parent" android:layout_width="fill_parent"
			android:layout_weight="1" android:orientation="vertical">
			<LinearLayout android:orientation="horizontal"
				android:layout_height="50dip" android:layout_weight="0"
				android:id="@+id/linearLayout4" android:layout_width="fill_parent"
				android:weightSum="4">
				<include android:layout_height="fill_parent" layout="@layout/detail_button_back"
					android:layout_width="0dp" android:layout_weight="1"></include>
				<include android:layout_height="fill_parent"
					layout="@layout/detail_button_report" android:layout_width="0dp"
					android:layout_weight="1"></include>
				<include android:layout_height="fill_parent"
					layout="@layout/detail_button_favorite" android:layout_width="0dp"
					android:layout_weight="1"></include>
				<include android:layout_height="fill_parent" layout="@layout/detail_button_chat"
					android:layout_width="0dp" android:layout_weight="1"></include>
			</LinearLayout>
			<include layout="@layout/user_profile"></include>
			<Gallery android:id="@+id/gallery" android:background="@android:color/black"
			android:layout_height="90dip" android:layout_weight="0"
			android:layout_width="fill_parent"></Gallery>
		</LinearLayout>
		<include android:layout_height="fill_parent" layout="@layout/map"
			android:layout_width="fill_parent" android:layout_weight="1"></include>
	</LinearLayout>
</LinearLayout>