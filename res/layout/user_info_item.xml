<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/user_info_item"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	xmlns:android="http://schemas.android.com/apk/res/android">
			<ImageView android:id="@+id/user_info_icon"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:paddingTop="6dip" android:layout_marginRight="12dip"/>
		<TextView android:text="" android:id="@+id/user_info_description"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			style="@android:style/TextAppearance.Small" android:layout_toRightOf="@id/user_info_icon"></TextView>
</RelativeLayout>
